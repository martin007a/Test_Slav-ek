<Window x:Class="Test_Slavíček.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test_Slavíček"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Button Name="btn_Jet" Content="Jet" HorizontalAlignment="Center" Height="40" Grid.Row="4" VerticalAlignment="Center" Width="100" Click="btn_Jet_Click"/>
        <Button Name="btn_Naložit" Grid.Column="1" Content="Naložit" HorizontalAlignment="Stretch" Height="40" Grid.Row="4" VerticalAlignment="Center" Width="100" Click="btn_Naložit_Click"/>
        <Button Name="btn_Tankovat" Grid.Column="2" Content="Natankovat" HorizontalAlignment="Center" Height="40" Grid.Row="4" VerticalAlignment="Center" Width="100" Click="btn_Tankovat_Click"/>
        <TextBox Name="txtbox1" Grid.ColumnSpan="2" Margin="50,10,169,36" Grid.RowSpan="4" TextWrapping="Wrap" Text="TextBox"/>
        <Button x:Name="btn_Vysipat" Grid.Column="3" Content="Vysipat" HorizontalAlignment="Center" Grid.Row="4" VerticalAlignment="Center" Height="40" Width="100" Click="btn_Vysipat_Click"/>

    </Grid>
</Window>
